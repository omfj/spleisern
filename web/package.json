{
	"name": "@spleis/web",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "pnpm run build && wrangler dev",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"lint:fix": "pnpm format && eslint . --fix",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "wrangler d1 migrations apply spleis-db --local",
		"db:migrate:prod": "wrangler d1 migrations apply spleis-db --remote",
		"deploy": "pnpm run build && wrangler deploy",
		"cf-typegen": "wrangler types ./src/worker-configuration.d.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.32.0",
		"@playwright/test": "^1.54.2",
		"@sveltejs/adapter-cloudflare": "^7.1.2",
		"@sveltejs/kit": "^2.27.0",
		"@sveltejs/vite-plugin-svelte": "^6.1.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@types/node": "^22.16.0",
		"@vitest/browser": "^3.2.3",
		"drizzle-kit": "^0.31.4",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"playwright": "^1.54.2",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.37.3",
		"svelte-check": "^4.3.1",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.39.0",
		"vite": "^7.0.6",
		"vite-plugin-devtools-json": "^0.4.1",
		"vitest": "^3.2.3",
		"vitest-browser-svelte": "^1.1.0",
		"wrangler": "^4.28.0"
	},
	"dependencies": {
		"@lucide/svelte": "^0.536.0",
		"@mistralai/mistralai": "^1.7.5",
		"@oslojs/oauth2": "^0.5.0",
		"arctic": "^3.7.0",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"drizzle-orm": "^0.44.4",
		"jose": "^6.0.12",
		"nanoid": "^5.1.5",
		"runed": "^0.31.1",
		"tailwind-merge": "^3.3.1",
		"zod": "^3.25.75"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"packageManager": "pnpm@9.15.9+sha512.68046141893c66fad01c079231128e9afb89ef87e2691d69e4d40eee228988295fd4682181bae55b58418c3a253bde65a505ec7c5f9403ece5cc3cd37dcf2531"
}
